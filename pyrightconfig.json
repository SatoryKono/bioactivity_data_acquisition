{
  "include": [
    "src",
    "tools/typecheck/stubs"
  ],
  "exclude": [
    "**/node_modules",
    "**/__pycache__",
    "**/.*"
  ],
  "stubPath": "tools/typecheck/stubs",
  "typeCheckingMode": "strict",
  "reportMissingImports": true,
  "reportMissingTypeStubs": false,
  "pythonVersion": "3.10",
  "venvPath": ".",
  "venv": ".venv"
}
