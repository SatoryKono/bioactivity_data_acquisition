+"""Utilities for module-level export helpers."""
+
+from __future__ import annotations
+
+from collections.abc import Iterable
+
+
+def sorted_exports(exports: Iterable[str]) -> list[str]:
+    """Return deterministic ordering for ``__dir__`` implementations."""
+
+    return sorted(set(exports))
